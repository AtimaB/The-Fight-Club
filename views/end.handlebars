<div class="container">
    <div class="row">
        <div class="col s12" id="highScore"><span class="flow-text">Highscore</span></div>
         <p> Player's name: {{}}</p><br>
        <p>score: {{}}</p>
    </div>
    <div class="row">
        <table>
        <thead>
          <tr>
              <th>Name</th>
              <th>Score</th>
          </tr>
        </thead>

        <tbody>
          <tr>
            <td>{{ name}}</td>
            <td>{{score}}</td>
          </tr>
        </tbody>
      </table>
    </div>

    <!-- start game button !-->
   <button type="submit" class="start btn-large col s4 offset-s4" id="start">Play Again!</button>
</div>